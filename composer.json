    "type": "library",
    "keywords": [
        "open-admin",
        "wysiwyg",
        "extension",
        "editor",
        "quill"
    ],
    "homepage": "https://github.com/open-admin-ext/quill",
    "license": "MIT",
    "authors": [
        {
            "name": "open-admin",
            "email": "info@open-admin.org"
        }
    ],
    "require": {
        "php": ">=7.0.0",
