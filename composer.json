        "open-admin-org/open-admin": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAdmin\\Admin\\Quill\\": "src/"
        }
